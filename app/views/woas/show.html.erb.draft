<!-- Rating mean + comment -->

  <% if @booking.first != nil%>
    <% @woa.booking do |dose| %>
    <% if dose.persisted? %>
      <li>
        <%= dose.description %> <%= dose.ingredient.name %>
      </li>
    <% end %>
  <% end %>
</ul>

